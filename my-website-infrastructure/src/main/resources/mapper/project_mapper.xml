<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luo.infrastructure.dao.ProjectDao">

    <select id="queryProjects" resultType="com.luo.infrastructure.model.Project">
        select id,
               title,
               photo,
               description,
               front_technology,
               backend_technology,
               create_time,
               update_time
        from projects
    </select>
</mapper>
